});\n        }else if (status === ORDER_REJECTED){\n            await contract.methods.rejectOrder()\n        } else if (status === ORDER_CANCELLED){\n            await contract.methods.cancelOrder();\n        }\n        return await fetchDataFromHash(contract)\n    } catch(e){\n        console.log(e);\n    }\n}","/Users/coolatmusic/Documents/Practice/smart-contract/pentonium/webapp/src/config/abi/orderContract.js",[],{"ruleId":"401","severity":1,"message":"402","line":2,"column":27,"nodeType":"403","messageId":"404","endLine":2,"endColumn":33},{"ruleId":"401","severity":1,"message":"405","line":2,"column":42,"nodeType":"403","messageId":"404","endLine":2,"endColumn":46},{"ruleId":"406","severity":1,"message":"407","line":32,"column":24,"nodeType":"408","messageId":"409","endLine":32,"endColumn":26},{"ruleId":"406","severity":1,"message":"407","line":35,"column":25,"nodeType":"408","messageId":"409","endLine":35,"endColumn":27},{"ruleId":"406","severity":1,"message":"410","line":48,"column":12,"nodeType":"408","messageId":"409","endLine":48,"endColumn":14},{"ruleId":"401","severity":1,"message":"411","line":68,"column":7,"nodeType":"403","messageId":"404","endLine":68,"endColumn":12},{"ruleId":"412","severity":1,"message":"413","line":9,"column":1,"nodeType":"414","endLine":14,"endColumn":2},{"ruleId":"401","severity":1,"message":"402","line":3,"column":20,"nodeType":"403","messageId":"404","endLine":3,"endColumn":26},{"ruleId":"415","replacedBy":"416"},{"ruleId":"417","replacedBy":"418"},{"ruleId":"401","severity":1,"message":"402","line":3,"column":20,"nodeType":"403","messageId":"404","endLine":3,"endColumn":26},{"ruleId":"406","severity":1,"message":"410","line":27,"column":35,"nodeType":"408","messageId":"409","endLine":27,"endColumn":37},{"ruleId":"401","severity":1,"message":"402","line":2,"column":27,"nodeType":"403","messageId":"404","endLine":2,"endColumn":33},{"ruleId":"401","severity":1,"message":"405","line":2,"column":42,"nodeType":"403","messageId":"4